<!DOCTYPE html>
<html>
<head>
  <title>Taylor Medical Associates</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>

  <%#= javascript_include_tag "jquery", "data-turbolinks-track" => true %>
  
  <% set_meta_tags :keywords => %w[Family Physician Julie Taylor MD Medical Associates Chicago Pediatric Care   Doctor South Side Geriatrics Family Planning Advocate Trinity Hospital ] %>

  <%= csrf_meta_tags %>

</head>
<body>


<%= render "layouts/header" %>
<% if flash[:notice] %>
  <p id="flash_notice" class="messages notice"><%= flash[:notice] %></p>
  <script type="text/javascript">
    $(function() {
      $('p#flash_notice').delay(5000).fadeOut('slow');
    });
  </script>
<% end %>
<% if flash[:alert] %>
  <p id="flash_errors" class="messages alert"><%= flash[:alert] || flash[:alert] %></p>
  <script type="text/javascript">
    $(function() {
      $('p#flash_errors').delay(5000).fadeOut('slow');
    });
  </script>
<% end %>
<% flash[:alert] = flash[:alert] = flash[:alert] = nil %>

<%= yield %>

<%= render "layouts/footer" %>


</body>

<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-43764260-2', 'taylormedassc.com');
  ga('send', 'pageview');

</script>

</html>


